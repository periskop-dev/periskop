{
  "name": "periskop",
  "version": "0.0.1",
  "description": "Exception Aggregator for micro-service environments",
  "main": "index.js",
  "scripts": {
    "clean:all": "npm cache clean && npm run clean:dist && rimraf node_modules",
    "clean:dist": "rimraf dist && rimraf lint-reports",
    "build:dev": "npm run clean:dist && webpack --env=dev --progress --profile --colors --display-error-details --display-cached",
    "build:prod": "npm run clean:dist && npm run test && webpack --env=prod --progress --profile --colors --display-error-details --display-cached",
    "start": "webpack-dev-server --env=dev --progress --profile --colors",
    "test": "karma start --no-auto-watch --single-run",
    "test:watch": "karma start --auto-watch --no-single-run",
    "build:dist": "webpack --env=prod"
  },
  "author": "SoundCloud",
  "license": "Apache-2.0",
  "dependencies": {
    "bootstrap": "~3.4.1",
    "moment": "^2.22.2",
    "react": "~15.6.2",
    "react-bootstrap": "^0.32.1",
    "react-dom": "~15.4.2",
    "react-redux": "^5.1.2",
    "react-router": "^4.3.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/jasmine": "~3.5.0",
    "@types/node": "~6.0.60",
    "@types/react": "~16.9.9",
    "@types/react-addons-test-utils": "~0.14.17",
    "@types/react-bootstrap": "^0.32.20",
    "@types/react-dom": "~16.9.4",
    "@types/react-redux": "^6.0.6",
    "@types/react-router": "^4.0.30",
    "css-loader": "^3.1.0",
    "extract-text-webpack-plugin": "~2.1.0",
    "file-loader": "~0.10.1",
    "html-webpack-plugin": "~2.28.0",
    "jasmine-core": "~2.5.2",
    "karma": "^4.2.0",
    "karma-cli": "~2.0.0",
    "karma-jasmine": "~1.1.0",
    "karma-mocha-reporter": "~2.2.3",
    "karma-phantomjs-launcher": "~1.0.4",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~2.0.3",
    "less": "^3.10.3",
    "less-loader": "~4.0.2",
    "npm": "^6.13.4",
    "phantomjs-prebuilt": "~2.1.14",
    "react-addons-test-utils": "~15.6.2",
    "rimraf": "~2.6.1",
    "source-map-loader": "~0.2.0",
    "style-loader": "~0.23.1",
    "ts-loader": "~6.2.1",
    "ts-node": "~8.5.4",
    "tslint": "~4.4.2",
    "tslint-loader": "~3.4.3",
    "typescript": "3.7.3",
    "url-loader": "^2.1.0",
    "webpack": "~2.2.1",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.1.11",
    "webpack-merge": "~4.2.2"
  }
}
